<div class="table-responsive" data-filter-response>
	<div class="dataTables_wrapper">
		{{ include('blocks/paginate.html.twig', {'numPages': paginator.numPages, 'numResults': paginator.numResults}) }}

		<div class="row">
			<div class="col-sm-12">

				<table class="table table-bordered" width="100%" cellspacing="0">
					<thead>
						<tr>
							<th>{% trans %}Code{% endtrans %}</th>
							<th>{% trans %}Active{% endtrans %}</th>
							<th>{% trans %}Title{% endtrans %}</th>
							<th>{% trans %}Name{% endtrans %}</th>
							<th>{% trans %}Price{% endtrans %}</th>						
						</tr>
					</thead>
					<tbody>
					{% for shippingMethod in paginator.results %}
						<tr>
							<td>{{ shippingMethod.code }}</td>
							<td>{{ shippingMethod.active }}</td>
							<td>{{ shippingMethod.title }}</td>
							<td>{{ shippingMethod.name }}</td>
							<td>{{ shippingMethod.price }}</td>
						</tr>
					{% endfor %}
					</tbody>
				</table>
			</div>
		</div>
	</div>
</div>